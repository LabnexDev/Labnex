{
  "name": "frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:no-check": "vite build",
    "postbuild": "node scripts/generate-sitemap.js && node scripts/prerender-spa.js && node scripts/generate-og-images.js && node scripts/generate-og-screenshot.js && node scripts/generate-pwa-icons.js",
    "analyze": "node scripts/analyze-bundle.js",
    "analyze:simple": "node scripts/simple-bundle-analyzer.js",
    "analyze:imports": "node scripts/optimize-heavy-imports.js",
    "auto-optimize": "node scripts/auto-optimize.js",
    "replace-markdown": "node scripts/replace-markdown.js",
    "performance": "node scripts/performance-optimizer.js",
    "quick-check": "node scripts/quick-bundle-check.js",
    "wall-check": "node scripts/wall-check.js",
    "generate-pwa-icons": "node scripts/generate-pwa-icons.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "deploy": "npm run build && gh-pages -d dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "optimize": "node scripts/optimize-bundle.js",
    "deps:check": "npx depcheck",
    "lighthouse": "npx lighthouse http://localhost:5173 --output=html --output-path=./lighthouse-report.html --chrome-flags='--headless'",
    "lighthouse:ci": "npx lighthouse http://localhost:5173 --output=json --output-path=./lighthouse-report.json --chrome-flags='--headless' --only-categories=performance"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.3",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.1.1",
    "@tanstack/react-query": "^5.79.0",
    "@types/animejs": "^3.1.13",
    "@types/dompurify": "^3.0.5",
    "@types/lodash": "^4.17.16",
    "@types/marked": "^5.0.2",
    "animejs": "^4.0.2",
    "axios": "^1.9.0",
    "chart.js": "^4.5.0",
    "date-fns": "^3.6.0",
    "dompurify": "^3.2.6",
    "framer-motion": "^12.12.1",
    "gsap": "^3.13.0",
    "html2canvas": "^1.4.1",
    "marked": "^15.0.12",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.57.0",
    "react-hot-toast": "^2.5.2",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.6.0",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.1",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@resvg/resvg-js": "^2.3.0",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@types/chart.js": "^2.9.41",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-basic-ssl": "^2.0.0",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "playwright-core": "^1.53.0",
    "postcss": "^8.4.35",
    "satori": "^0.10.0",
    "start-server-and-test": "^2.0.12",
    "tailwindcss": "^3.4.1",
    "terser": "^5.43.1",
    "ts-jest": "^29.3.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-bundle-analyzer": "^0.23.0",
    "vite-plugin-compression": "^0.5.1"
  }
}
