{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2020",
    "outDir": "dist",
    "rootDir": "src",
    "baseUrl": "src",
    "paths": {
      "@labnex/executor": ["index"],
      "@labnex/executor/*": ["*"]
    },
    "strict": true,
    "esModuleInterop": true,
    "types": ["node"],
    "composite": true,
    "moduleResolution": "node"
  },
  "include": ["src"],
  "exclude": [
    "**/*.test.ts",
    "**/*.spec.ts"
  ]
}
